{
  "manifest_version": 3,
  "name": "OpenTrace Extension",
  "version": "1.0.1",

  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icons": {
      "16": "assets/icons16.png",
      "48": "assets/icons48.png",
      "128": "assets/icons128.png"
    }
  }
}
